<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yuri.sbt0207">

    <application
        android:allowBackup="true"
        android:icon="@drawable/mu"
        android:label="SBT"
        android:roundIcon="@drawable/mu"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".FeedbackActivity"></activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".IntroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TwoActivity" />
        <activity android:name=".ThreeActivity" />
        <activity android:name=".FourActivity" />
        <activity android:name=".FiveActivity" />
        <activity android:name=".LastActivity" />
        <service android:name=".MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>